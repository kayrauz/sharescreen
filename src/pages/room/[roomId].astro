---
import Layout from '../../layouts/Layout.astro';
import App from '../../components/App.tsx';

export const prerender = false;

const { roomId } = Astro.params;
---

<Layout 
  title={`ShareScreen - Room ${roomId}`}
  description={`Join room ${roomId} to view a shared screen.`}
>
  <App client:load initialScreen="guest" />
</Layout> 